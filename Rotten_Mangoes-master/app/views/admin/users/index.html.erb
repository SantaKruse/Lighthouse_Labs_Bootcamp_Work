<h1>Rotten Mangoes Users</h1>
<%= link_to "Back to all movies", movies_path %><br/>
<%= paginate @users %>
<hr>
<% @users.each do |user| %>
  <h2><%=user.full_name%></h2>
  <p>Profile type: <%=user.profile_type%></p>
  <%= link_to "Edit this user account", edit_admin_user_path(user) %>
  <%= link_to 'Delete User', admin_user_path(user),method: :delete, data: { confirm: 'Are you sure?' } %>
  <hr>
<% end %>

